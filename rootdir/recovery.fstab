# Android fstab file.
#<src>           <mnt_point>         <type>    <mnt_flags and options>                 <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
/dev/block/mmcblkp06       /system         ext4    ro,commit=1,noauto_da_alloc				   wait
/dev/block/mmcblkp08       /data           ext4    noatime,nosuid,nodev,noauto_da_alloc,discard	wait,check
/dev/block/mmcblkp07       /cache          ext4    noatime,nosuid,nodev,noauto_da_alloc,discard	wait,check
/dev/bootimg               /boot       	   emmc  		defaults                      defaults
/dev/recovery  	           /recovery   	   emmc  		defaults                      defaults
/devices/platform/mtk-msdc.0/mmc_host            auto        vfat  defaults              voldmanaged=sdcard0:9,nonremovable,noemulatedsd
/devices/platform/mtk-msdc.1/mmc_host            auto        vfat  defaults              voldmanaged=sdcard1:auto,noemulatedsd


